class Solution {
    public int countElements(int[] nums) {
        int min = nums[0];
        int max = nums[0];
        int count = 0;

        for(int e: nums){
            min = Math.min(e, min);
            max = Math.max(e, max);
        }

        for(int e: nums){
            if(e>min && e<max)
                count++;
        }
        return count;
    }
}
